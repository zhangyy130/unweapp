view {
    display: block;
}

text {
    display: block;
}

.container {
    position: absolute;
    top: 0;
    bottom: 0;
    overflow: hidden;
    width: 100%;
    padding: 10rpx;
    box-sizing: border-box;
}

canvas {
    position: absolute;
    width: 95%;
    height: 300rpx;
    margin: 0 auto;
    top: 150rpx;
    left: 19rpx;
}

.container .num {
    color: #fd7d3d;
    margin-top: 10rpx;
    font-weight: bold;
}

.indexes-con {
    overflow: hidden;
}

.indexes-item {
    width: 50%;
    box-sizing: border-box;
    float: left;
    text-align: center;
    flex: 1;
    font-size: 30rpx;
    color: #666;
    height: 130rpx;
}

.indexes-con .more-icon {
    position: absolute;
    top: 25rpx;
    right: 50rpx;
    display: inline;
    border: 10rpx solid black;
    border-left-color: transparent;
    border-right-color: transparent;
    border-bottom-color: transparent;
}

.explain-jump {
    position: absolute;
    top: 420rpx;
    color: #999;
    font-size: 28rpx;
    display: inline-block;
    padding: 40rpx 20rpx 20rpx;
}

.update-info {
    top: 520rpx;
    position: absolute;
    overflow: hidden;
    width: 100%;
    color: #888;
    background-color: #eee;
    font-size: 24rpx;
    padding: 10rpx 20rpx;
    box-sizing: border-box;
    box-shadow: 0 2px 5px #888;
}

.update-info .desc {
    float: left;
}

.update-info .date {
    float: right;
}

.invest-btn {
    position: fixed;
    width: 100%;
    bottom: 0;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    border-top: 1px solid #eee;
    box-shadow: 0 -8rpx 4px #eee;
    background-color: #fff;
    color: #fd7d3d;
    font-weight: bold;
}

.funds-list {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    top: 580rpx;
    overflow-y: scroll;
    padding-bottom: 100rpx;
}

.fund {
    overflow: hidden;
    padding: 30rpx 20rpx;
    font-size: 30rpx;
    color: #666;
    border-bottom: 1px solid #eee;
}

.fund .name {
    float: left;
}

.fund .index-num {
    float: right;
}

.fund.chosen {
    background-color: #fd7d3d;
    color: #fff;
}

.loading2-tips {
    position: absolute;
    top: 700rpx;
    text-align: center;
    font-size: 28rpx;
    color: #999;
    margin-top: 40rpx;
    left: 50%;
    transform: translateX(-50%);
}

.loading1-tips {
    position: absolute;
    top: 120rpx;
    left: 50%;
    transform: translateX(-50%);
    text-align: center;
    font-size: 28rpx;
    color: #999;
    margin-top: 40rpx;
}

.explain {
    margin-top: 10rpx;
    padding-bottom: 80rpx;
}

.explain-text {
    padding: 10rpx;
    color: #999;
    font-size: 24rpx;
}

.help-section {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: rgba(0,0,0,0.6);
    z-index: 100;
}

.help-pic {
    z-index: 100;
    background-color: #fff;
    width: 80%;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
}

.help-section .close {
    font-size: 80rpx;
    color: #fff;
    position: absolute;
    left: 50%;
    transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    top: 50%;
    margin-top: 350rpx;
}

.check {
    text-decoration: underline;
}